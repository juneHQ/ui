{
  "name": "@june-so/ui",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/ui.js",
  "exports": {
    ".": {
      "import": "./dist/ui.js",
